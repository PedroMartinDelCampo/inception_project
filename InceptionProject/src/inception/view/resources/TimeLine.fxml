<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="timelineGridPane" prefHeight="150.0" prefWidth="805.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inception.controller.TimelineController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="244.0" minWidth="10.0" prefWidth="95.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="713.0" minWidth="0.0" prefWidth="710.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Label text="Nombre" GridPane.rowIndex="1" />
      <Polyline fill="CHARTREUSE" scaleX="0.2" scaleY="0.2" strokeType="INSIDE" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <points>
          <Double fx:value="-50.0" />
          <Double fx:value="-40.0" />
          <Double fx:value="50.0" />
          <Double fx:value="-40.0" />
          <Double fx:value="0.0" />
          <Double fx:value="60.0" />
          <Double fx:value="-50.0" />
          <Double fx:value="-40.0" />
        </points>
         <rotationAxis>
            <Point3D />
         </rotationAxis>
         <GridPane.margin>
            <Insets left="-50.0" />
         </GridPane.margin>
      </Polyline>
      <Button fx:id="deleteTimelineButton" mnemonicParsing="false" onAction="#deleteTimeline" text="Delete Timeline" GridPane.columnIndex="1">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#addTimeline" text="New Timeline" />
   </children>
</GridPane>
